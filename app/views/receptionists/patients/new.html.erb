<h1>Register New Patient</h1>

<%= form_with model: @patient, url: receptionists_patients_path, local: true do |f| %>

  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :age %>
    <%= f.number_field :age %>
  </div>

  <div>
    <%= f.label :contact %>
    <%= f.text_field :contact_info %>
  </div>
  <div>
    <%= f.submit 'Register' %>
  </div>
<% end %>

<%= link_to 'Back', @patients %>
